    "test2": {
        "displayName": "Sizmek",
        "fields": {
            "campaignType": {
                "displayName": "Campaign Type",
                "dependent": null,
                "type": "dropdown",
                "data": "campaigntype.json",
                "placeholder": "Select Campaign Type",
                "width": 6
            },
            "something": {
                "displayName": "Something",
                "width": 6,
                "dependent": null,
                "type": "dropdown",
                "data": "month.json"
            },
            "month": {
                "displayName": "Month",
                "width": 3,
                "dependent": null,
                "type": "dropdown",
                "data": "month.json"
            },
            "lol": {
                "displayName": "Testing",
                "width": 3,
                "dependent": null,
                "type": "dropdown",
                "data": "month.json"
            },
            "year": {
                "displayName": "Year",
                "width": 3,
                "dependent": null,
                "type": "dropdown",
                "data": "year.json"
            },
            "campaignName": {
                "displayName": "Campaign Name",
                "dependent": "campaignType",
                "show": false,
                "type": "case",
                "width": 4,
                "case": {
                    "Local": {
                        "placeholder": "Enter Local Campaign",
                        "type": "manual",
                        "regex": "[A-Z]*",
                        "prefix": "L_",
                        "suffix": "LOL"
                    },
                    "Global": {
                        "placeholder": "Enter Global Campaign",
                        "type": "dropdown",
                        "data": "campaigns.json"
                    }
                }
            }
        },
        "output": {
            "dependent": "campaignType",
            "case": {
                "Local": {
                    "encode": false,
                    "format": "L_[campaignType]-[month][year]--[campaignName]"
                },
                "Global": {
                    "encode": true,
                    "format": "[campaignType]-[month]-[year]-campaign is [campaignName]"
                }
            }
        }
    },
